{
  "name": "faiyt-astal-shell",
  "version": "1.0.0",
  "description": "UI Recreation for AGS/Hyprland",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:app": "vite build",
    "build": "bun build:app",
    "start:app": "gjs -m dist/main.js",
    "debug:app": "GTK_DEBUG=interactive yarn start:app",
    "start": "yarn build && yarn start:app",
    "validate": "yarn validate:types",
    "validate:types": "tsc --noEmit",
    "clear": "rm -rf dist @types"
  },
  "keywords": [
    "ags",
    "typescript",
    "gtk"
  ],
  "author": "Dane L Miller",
  "dependencies": {
    "@girs/gjs": "^4.0.0-beta.23",
    "@girs/gtk-4.0": "^4.18.3-4.0.0-beta.23",
    "astal": "/usr/share/astal/gjs",
    "csstype": "^3.1.3"
  }
}
